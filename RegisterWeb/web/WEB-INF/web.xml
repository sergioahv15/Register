<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>CursosServlet</servlet-name>
        <servlet-class>Register.controller.CursosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarrerasServlet</servlet-name>
        <servlet-class>Register.controller.CarrerasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfesoresServlet</servlet-name>
        <servlet-class>Register.controller.ProfesoresServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstudiantesServlet</servlet-name>
        <servlet-class>Register.controller.EstudiantesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CiclosServlet</servlet-name>
        <servlet-class>Register.controller.CiclosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OfertaServlet</servlet-name>
        <servlet-class>Register.controller.OfertaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GruposProfeServlet</servlet-name>
        <servlet-class>Register.controller.GruposProfeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistorialServlet</servlet-name>
        <servlet-class>Register.controller.HistorialServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CursosServlet</servlet-name>
        <url-pattern>/CursosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarrerasServlet</servlet-name>
        <url-pattern>/CarrerasServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfesoresServlet</servlet-name>
        <url-pattern>/ProfesoresServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstudiantesServlet</servlet-name>
        <url-pattern>/EstudiantesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CiclosServlet</servlet-name>
        <url-pattern>/CiclosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OfertaServlet</servlet-name>
        <url-pattern>/OfertaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GruposProfeServlet</servlet-name>
        <url-pattern>/GruposProfeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistorialServlet</servlet-name>
        <url-pattern>/HistorialServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.xhtml</welcome-file>
    </welcome-file-list>
</web-app>
